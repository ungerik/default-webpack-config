{
	"env": {
		"node": true
	},
	"rules": {
		"indent": [
			2,
			"tab"
		],
		"quotes": [
			1,
			"double"
		],
		"linebreak-style": [
			2,
			"unix"
		],
		"semi": [
			2,
			"always"
		],

		"strict": 0,
		"no-underscore-dangle": 0,
		"no-alert": 1,
		"camelcase": 1,
		"no-console": 0,

		"no-undef": 1,
		"no-unused-vars": 1,
		"no-label-var": 1,
		"no-delete-var": 1,
		"consistent-return": 1,
		"no-dupe-keys": 1,
		"no-extra-semi": 1,
		"no-func-assign": 1,
		"no-obj-calls": 1,
		"no-unreachable": 1,
		"use-isnan": 1,
		"valid-typeof": 1,
		"no-invalid-regexp": 1,

		"constructor-super": 1,
		"no-const-assign": 1,
		"no-dupe-class-members": 1,
		"prefer-const": 1,
	}
}
